{
    "name": "codeportal/php-url-shortner",
    "description": "Url Shortener for Laravel. We're using cdpt.in platform to shorten your URLs. As this is open source, you don't need any kind of API key to shorten your URL.",
    "keywords": [
        "codeportal",
        "laravel",
        "url",
        "shortener",
        "url short",
        "url shortener"
    ],
    "homepage": "https://cdpt.in",
    "license": "MIT",
    "authors": [
        {
            "name": "Harsh Doshi",
            "email": "harshdoshi999@gmail.com",
            "homepage": "https://harshdoshi.in",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "5.1.x|5.2.x|5.3.*|5.4.*|5.5.*"
    },
    "autoload": {
        "psr-4": {
            "CodePortal\\UrlShortener\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodePortal\\UrlShortener\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev"
}